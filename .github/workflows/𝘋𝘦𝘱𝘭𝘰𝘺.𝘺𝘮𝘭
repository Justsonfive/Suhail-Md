Name: Node.js CI
on:
 push:
  branches:
   - main
 pull_request:
  branches:
    - main

jobs:
  build:
  
   runs-on: buntu-latest


     strategy:
       matrix:
        node-version: [20.x]
steps:
- name: Checkout repository uses: actions/checkout@v3
- name: Set up Node. js uses: actions/setup-node@v3 with:
node-version: S{f matrix.node-version })
- name: Install dependencies run: nom install
- name: Start application 
run: pm start
